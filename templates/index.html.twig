{% extends 'base.html.twig' %}

{% block title %}
	Users
{% endblock %}

{% block body %}
	<h1 class='bg-red-900 text-6xl'>Users</h1>
	<table>
		<tr>
			<th>
				No
			</th>
			<th>
				Name
			</th>
			<th>
				Email
			</th>
			<th>
				Age
			</th>
		</tr>
		{% for student in students %}

			<tr>
				<td>{{student.id}}</td>
				<td>{{student.name}}</td>
				<td>{{student.email}}</td>
				<td>{{student.age}}</td>
			</tr>
		{% endfor %}
	</table>
{% endblock %}
